{
	"name": "@nikitau/eslint",
	"type": "module",
	"description": "eslint configs",
	"license": "MIT",
	"version": "2.0.2",
	"keywords": [
		"eslint",
		"react",
		"node"
	],
	"author": {
		"name": "Nikita Utyugov",
		"url": "https://github.com/nikitauty"
	},
	"bugs": {
		"url": "https://github.com/nikitauty/core/issues"
	},
	"homepage": "https://github.com/nikitauty/core",
	"repository": {
		"url": "https://github.com/nikitauty/core"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "index.js",
	"types": "index.d.ts",
	"files": [
		"index.d.ts",
		"index.js"
	],
	"scripts": {
		"format": "prettier --write \"*.js\""
	},
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"eslint --fix"
		],
		"*.ts": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"peerDependencies": {
		"eslint": "^8.48.0"
	},
	"dependencies": {
		"@antfu/eslint-config": "^2.25.1",
		"@eslint-react/eslint-plugin": "^1.10.0",
		"@next/eslint-plugin-next": "^14.2.5",
		"@vue/compiler-sfc": "^3.4.38",
		"eslint": "^9.9.0",
		"eslint-plugin-jsx-a11y": "^6.9.0",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.9",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"@typescript-eslint/eslint-plugin": "*"
	},
	"devDependencies": {
		"@nikitau/prettier": "*"
	}
}
